{
    "name": "instante/php-test-suite",
    "type": "library",
    "description": "Instante unit and integration test suite",
    "keywords": [
        "nette",
        "instante"
    ],
    "license": [
        "BSD-3-Clause",
        "GPL-2.0",
        "GPL-3.0"
    ],
    "require": {
        "php": ">=5.5.0",
        "nette/tester": "@dev",
        "nette/bootstrap": "~2.2"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "~0.6",
        "nette/robot-loader": "~2.2",
        "nette/caching": "~2.2"
    },
    "suggest": {
        "nette/robot-loader": "~2.2",
        "nette/caching": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Instante\\Tests\\": "src/"
        }
    },
    "bin": [
        "src/test-bootstrap.php"
    ]
}
